@model CamposRepresentacoes.Models.PedidoViewModel
@{

    string logoBase64 = "data:image/png;base64,";
}

<div class="container-fluid mt-4" style="margin-top: 100px">
    @if (Model.ProdutosPedido != null)
    {
        <div class="col-md-12">
            <div class="card shadow p-4">
                <div class="title-container">
                    <div class="card-body">
                        <div class="text-center mb-4">
                            @if (Model.Pedido.RazaoSocialFornecedor.Contains("MEGAVILLE INDUSTRIA"))
                            {
                                <img src="@logoBase64" alt="Logo" class="img-fluid" style="max-width: 200px;">
                            }
                        </div>
                        <div class="text-center mb-4">
                            <h2><strong>Informações do Pedido</strong></h2>
                        </div>
                        <div class="form-row">
                            <div class="col-md-6 mb-3">
                                <label for="cliente"><strong>Cliente:</strong></label>
                                <input type="text" class="form-control" id="cliente" value="@Model.Cliente.RazaoSocial" readonly>
                            </div>
                            <div class="col-md-3 mb-3">
                                <label for="cnpj"><strong>CNPJ:</strong></label>
                                <input type="text" class="form-control" id="cnpj" value="@Model.Cliente.CNPJ" readonly>
                            </div>
                            <div class="col-md-3 mb-3">
                                <label for="inscricaoEstadual"><strong>Insc. Estadual:</strong></label>
                                <input type="text" class="form-control" id="inscricaoEstadual" value="@Model.Cliente.InscricaoEstadual" readonly>
                            </div>
                            <div class="col-md-6 mb-3">
                                <label for="email"><strong>Email:</strong></label>
                                <input type="text" class="form-control" id="email" value="@Model.Cliente.Email" readonly>
                            </div>
                            <div class="col-md-3 mb-3">
                                <label for="telefone"><strong>Telefone:</strong></label>
                                <input type="text" class="form-control" id="telefone" value="@Model.Cliente.Telefone" readonly>
                            </div>
                            <div class="col-md-3 mb-3">
                                <label for="celular"><strong>Celular:</strong></label>
                                <input type="text" class="form-control" id="celular" value="@Model.Cliente.Celular" readonly>
                            </div>
                            <div class="col-md-6 mb-3">
                                <label for="endereco"><strong>Endereço:</strong></label>
                                <input type="text" class="form-control" id="endereco" value="@($"{Model.Cliente.Rua}, {Model.Cliente.Numero}, {Model.Cliente.Bairro}, {Model.Cliente.Cidade} - BA")" readonly>
                            </div>
                            <div class="col-md-6 mb-3">
                                <label for="cep"><strong>CEP:</strong></label>
                                <input type="text" class="form-control" id="cep" value="@Model.Cliente.CEP" readonly>
                            </div>
                            <div class="col-md-6 mb-3">
                                <label for="fornecedor"><strong>Fornecedor:</strong></label>
                                <input type="text" class="form-control" id="fornecedor" value="@Model.Pedido.RazaoSocialFornecedor" readonly>
                            </div>
                            <div class="col-md-6 mb-3">
                                <label for="transportadora"><strong>Transportadora:</strong></label>
                                <input type="text" class="form-control" id="transportadora" value="@Model.Transportadora.RazaoSocial" readonly>
                            </div>
                            <div class="col-md-4 mb-3">
                                <label for="telefoneTransportadora"><strong>Telefone Transportadora:</strong></label>
                                <input type="text" class="form-control" id="telefoneTransportadora" value="@Model.Transportadora.Telefone" readonly>
                            </div>
                            <div class="col-md-4 mb-3">
                                <label for="prazo"><strong>Prazo:</strong></label>
                                <input type="text" class="form-control" id="prazo" value="@Model.Pedido.FormaPagamento" readonly>
                            </div>
                            <div class="col-md-4 mb-3">
                                <label for="observacao"><strong>Observação :</strong></label>
                                <textarea class="form-control" id="observacao" rows="3" readonly>@Model.Pedido.Observacao</textarea>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="card-body p-0 bg-principal">
                    <div class="card-body">
                        <table id="pedido-table" class="table table-sm table-bordered table-striped bg-white text-sm">
                            <thead>
                                <tr>
                                    <th>Código</th>
                                    <th>Descrição</th>
                                    <th>Valor Unitário</th>
                                    <th>Quantidade</th>
                                    <th>Valor Total</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model.ProdutosPedido)
                                {
                                    <tr>
                                        <td>@item.Codigo</td>
                                        <td>@item.Nome</td>
                                        <td>@string.Format("{0:C}", item.ValorUnitario)</td>
                                        <td>@item.TotalProduto</td>
                                        <td>@string.Format("{0:C}", item.ValorTotal)</td>
                                    </tr>
                                }
                                <tr>
                                    <td colspan="4" class="text-right"><strong>Total do Pedido:</strong></td>
                                    <td>@string.Format("{0:C}", Model.Pedido.ValorTotal)</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
                <div class="text-center">
                    <p><strong>Representante: </strong>Julio Campos</p>
                </div>
            </div>
        </div>

    }
    else if (Model.Pedido == null)
    {
        <div class="row mt-4">
            <div class="col-12">
                <div class="card shadow-sm">
                    <div class="card-body">
                        <p class="m-0 text-center text-muted"><i class="fas fa-info-circle mr-2"></i>Não foram encontrados resultados para sua pesquisa.</p>
                    </div>
                </div>
            </div>
        </div>
    }
</div>
